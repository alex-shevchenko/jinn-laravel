{
    "name": "jinn/laravel",
    "type": "library",
    "description": "Jinn for Laravel. Models, Migrations and API generator.",
    "keywords": ["jinn", "generator"],
    "homepage": "https://jinncode.dev/",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Shevchenko",
            "email": "caezaris@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../jinn-core"
        }
    ],
    "autoload": {
        "psr-4": {
            "Jinn\\Laravel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jinn\\Laravel\\JinnServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php" : ">=7.4",
        "laravel/framework": "^8.54|^9.0|^10.0",
        "jinn/core": "dev-master",
        "nette/php-generator": "^3.6"
    }
}
