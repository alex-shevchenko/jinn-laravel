{
    "name": "jinn/jinn-laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Shevchenko",
            "email": "caezaris@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../jinn-core"
        }
    ],
    "autoload": {
        "psr-4": {
